{
    "builds": [{
        "src": "smartreport/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "25mb" },
        "runtime": "python3.9"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "smartreport/wsgi.py"
        }
    ]
}